{
  "name": "playwright-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "test:ci": "npx playwright test --reporter=line",
    "test:update": "npx playwright test --update-snapshots",
    "docker": "docker-compose run --rm autotests",
    "docker:test": "npm run docker npm run test",
    "docker:debug": "npm run docker npm run test:debug",
    "docker:test:ci": "npm run docker npm run test:ci",
    "docker:update": "npm run docker npm run test:update",
    "docker:autotest:build": "docker build -t playwright-docker .",
    "docker:autotest": "docker run --rm -it playwright-docker",
    "docker:autotest:start": "npm run docker:autotest npm run test",
    "docker:autotest:update": "npm run docker:autotest npm run test:update",
    "docker:autotest:delete": "docker image rm playwright-docker"
  },
  "author": "Nitvex",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ketcher-core": "^2.7.2"
  },
  "lint-staged": {
    "*.{js,css,md,ts}": "prettier --write"
  },
  "types": "./tests/typings.d.ts"
}
