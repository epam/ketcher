{
  "name": "ketcher-react-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:standalone": "cross-env MODE=standalone react-app-rewired start",
    "start:remote": "cross-env MODE=remote react-app-rewired start",
    "clean:build": "shx rm -rf build",
    "clean:standalone": "shx rm -rf dist/standalone && shx mkdir -p dist/standalone",
    "clean:remote": "shx rm -rf dist/remote && shx mkdir -p dist/remote",
    "build:standalone": "cross-env MODE=standalone yarn clean:standalone && react-app-rewired build && shx cp -r build/. dist/standalone && yarn clean:build",
    "build:remote": "cross-env MODE=remote yarn clean:remote && react-app-rewired build && shx cp -r build/. dist/remote && yarn clean:build",
    "build": "run-s build:standalone build:remote",
    "build:standalone:analyze": "cross-env MODE=standalone react-app-rewired build --analyze",
    "build:remote:analyze": "cross-env MODE=remote react-app-rewired build --analyze",
    "test": "react-app-rewired test --passWithNoTests",
    "serve:remote": "serve -s dist/remote -l 4001",
    "serve:standalone": "serve -s dist/standalone -l 4002",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "ketcher-core": "workspace:*",
    "ketcher-react": "workspace:*",
    "ketcher-standalone": "workspace:*",
    "miew": "^0.9.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@welldone-software/why-did-you-render": "^4.3.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "^4.0.3",
    "serve": "^13.0.2",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.2",
    "typescript": "^4.4.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": "config/webpack.config.js"
}
