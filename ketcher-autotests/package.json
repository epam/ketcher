{
  "name": "playwright-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "report": "npx playwright show-report",
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "test:ci": "npx playwright test --reporter=line",
    "test:update": "npx playwright test --update-snapshots",
    "docker": "DOCKER=true docker-compose run --rm autotests",
    "docker:build": "docker-compose build autotests",
    "docker:test": "npm run docker npx playwright test",
    "docker:test:ci": "npm run docker npm run test:ci",
    "docker:update": "npm run docker npm run test:update",
    "docker:update:test": "npm run docker -- npx playwright test --update-snapshots -g"
  },
  "author": "Nitvex",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ketcher-core": "^2.7.2"
  },
  "lint-staged": {
    "*.{js,css,md,ts}": "prettier --write"
  },
  "types": "./tests/typings.d.ts"
}
